<ehcache>
    <diskStore path="java.io.tmpdir"/>
    <cache name="com.huateng.ebank.entity.DataDic" maxElementsInMemory="2000"
    	eternal="false" timeToLiveSeconds="7200" timeToIdleSeconds="3600" overflowToDisk="false"/>
    <cache name="com.huateng.ebank.entity.BranchInfo" maxElementsInMemory="2000"
    	eternal="false" timeToLiveSeconds="7200" timeToIdleSeconds="3600" overflowToDisk="false"/>
    <cache name="com.huateng.ebank.entity.Operator" maxElementsInMemory="2000"
    	eternal="false" timeToLiveSeconds="7200" timeToIdleSeconds="3600" overflowToDisk="false"/>
    <cache name="com.huateng.ebank.entity.FunctionInfo" maxElementsInMemory="2000"
    	eternal="false" timeToLiveSeconds="7200" timeToIdleSeconds="3600" overflowToDisk="false"/>
    <cache name="org.hibernate.cache.StandardQueryCache" maxElementsInMemory="50"
    	eternal="false" timeToIdleSeconds="3600" timeToLiveSeconds="7200" overflowToDisk="false"/>
    <cache name="org.hibernate.cache.UpdateTimestampsCache" maxElementsInMemory="5000"
    	eternal="true" overflowToDisk="false"/>
    <defaultCache maxElementsInMemory="10000" eternal="false" timeToIdleSeconds="120"
